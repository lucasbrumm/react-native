generator client {
  provider = "prisma-client-js"
  previewFeatures = ["reactNative"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./app.db"
}

model Recipe {
    id              Int     @id @default(autoincrement())
    name            String
    ingredients     String
    directions      String
    tested          Boolean @default(false)
}